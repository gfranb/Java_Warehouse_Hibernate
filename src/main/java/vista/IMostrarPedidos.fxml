<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="700.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controlador.IMostrarPedidos">
    <children>
        <Label layoutX="302.0" layoutY="119.0" text="Gestionar Pedidos" />
        <Button fx:id="btnEnviado" layoutX="219.0" layoutY="241.0" mnemonicParsing="false" onAction="#mostrarEnviado" prefHeight="25.0" prefWidth="107.0" text="Enviados" />
        <Button fx:id="btnPendiente" layoutX="219.0" layoutY="294.0" mnemonicParsing="false" onAction="#mostrarPendiente" prefHeight="25.0" prefWidth="107.0" text="Pendientes" />
        <Button fx:id="btnEnviado_C" layoutX="388.0" layoutY="294.0" mnemonicParsing="false" onAction="#mostrarEnviado_C" prefHeight="25.0" prefWidth="136.0" text="Enviados por Cliente" />
        <Button fx:id="btnPendiente_C" layoutX="388.0" layoutY="241.0" mnemonicParsing="false" onAction="#mostrarPendiente_C" text="Pendientes por Cliente" />
        <Button fx:id="btnVolver" layoutX="23.0" layoutY="21.0" mnemonicParsing="false" onAction="#volverEscena" prefHeight="25.0" prefWidth="54.0" style="-fx-border-color: black; -fx-background-color: transparent; -fx-border-radius: 50;" text="Volver" />
